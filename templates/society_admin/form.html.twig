{% trans_default_domain 'admin' %}

<div class="container">
    {{ form_start(form) }}

    <div class="row">
        <div class="col">
            {{ form_errors(form) }}
        </div>     
    </div>
    
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.name) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.motto) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.emailPublic) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.emailForm) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.phoneMobile) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.phoneFixe) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-5">
            {{ form_row(form.address) }}
        </div>
        <div class="col-md-3">
            {{ form_row(form.zipcode) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.city) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.facebookLink) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.twitterLink) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.youtubeLink) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.instagramLink) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.githubLink) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.linkedinLink) }}
        </div>
    </div>
    
        <input type="submit" class="btn btn-success" value="Enregistrer">
        {{ form_rest(form) }}
    {{ form_end(form) }}
</div>